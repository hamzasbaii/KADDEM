pipeline{
    agent any
    stages {
        stage("Testing Maven"){
            steps {
                sh "mvn -version"
            }
        }
        stage("Testing JDK"){
            steps {
                sh "java -version"
            }
        }
        stage("Chekout Git"){
            steps {
                echo "Pulling"
                    git branch: 'AhmedHachani5se2'
                    url: 'https://github.com/hamzasbaii/KADDEM'
                    credentialsId: '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'
            }
        }
        stage('MVN COMPILE') {
            steps{
                sh 'mvn compile'
        }
}

    }
}
